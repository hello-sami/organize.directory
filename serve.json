{
  "public": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/cities", "destination": "/cities.html" },
    { "source": "/issues", "destination": "/issues.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/cities/:city", "destination": "/cities/:city.html" }
  ]
}
