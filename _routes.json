{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_*"],
  "routes": [
    {
      "src": "/location",
      "dest": "/location.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/issues",
      "dest": "/issues.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/about",
      "dest": "/about.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/resources",
      "dest": "/resources.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/posts/:slug",
      "dest": "/posts/:slug.html"
    },
    {
      "src": "/cities/:name",
      "dest": "/cities/:name.html"
    },
    {
      "src": "/states/:name",
      "dest": "/states/:name.html"
    },
    {
      "src": "/issues/:name",
      "dest": "/issues/:name.html"
    }
  ]
} 