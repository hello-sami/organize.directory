{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_*"],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/location(\\.html)?$",
      "dest": "/location.html"
    },
    {
      "src": "^/issues(\\.html)?$",
      "dest": "/issues.html"
    },
    {
      "src": "^/about(\\.html)?$",
      "dest": "/about.html"
    },
    {
      "src": "^/resources(\\.html)?$",
      "dest": "/resources.html"
    },
    {
      "src": "^/posts/([^/]+)(\\.html)?$",
      "dest": "/posts/$1.html"
    },
    {
      "src": "^/cities/([^/]+)(\\.html)?$",
      "dest": "/cities/$1.html"
    },
    {
      "src": "^/states/([^/]+)(\\.html)?$",
      "dest": "/states/$1.html"
    },
    {
      "src": "^/issues/([^/]+)(\\.html)?$",
      "dest": "/issues/$1.html"
    }
  ]
} 