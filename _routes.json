{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_*"],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "src": "^/([^/]+)/?$",
      "dest": "/states/$1.html",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "Content-Type": "text/html; charset=utf-8",
        "Cache-Control": "no-transform"
      }
    }
  ]
} 