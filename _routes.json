{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_*"],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html",
      "headers": {
        "X-Content-Type-Options": "nosniff"
      }
    },
    {
      "src": "^/([^/]+)/?$",
      "dest": "/states/$1.html",
      "headers": {
        "X-Content-Type-Options": "nosniff"
      }
    }
  ]
} 